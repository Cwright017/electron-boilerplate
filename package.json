{
  "name": "Electron-Boilerplate",
  "version": "1.0.0",
  "description": "Electron boilerplate with ES6, SCSS and packaging.",
  "main": "main.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "start": "npm run build && NODE_ENV=dev electron ./dist/main.js",
    "dev": "./node_modules/.bin/webpack-dev-server --progress --port 3000 --config ./webpack.config.web.js --hot",
    "build": "npm run clean && webpack --progress --colors --config ./webpack.config.electron.js .",
    "package": "npm run build && cp ./package.json ./dist && electron-packager ./dist --platform=darwin --arch=x64 --prune --overwrite",
    "test": "mocha --require babel-core/register --require ignore-styles ./app/test/setup.js ./app/**/*.spec.js --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cwright017/electron-boilerplate.git"
  },
  "keywords": [
    "Electron",
    "boilerplate",
    "webpack",
    "react",
    "mocha",
    "scss",
    "sass",
    "electron-packager"
  ],
  "author": "Carl Wright",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cwright017/electron-boilerplate/issues"
  },
  "homepage": "https://github.com/Cwright017/electron-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.24.0",
    "electron-packager": "^7.7.0",
    "enzyme": "^2.4.1",
    "html-webpack-plugin": "^2.22.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9"
  },
  "dependencies": {
    "electron": "^1.3.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^1.0.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0"
  }
}
